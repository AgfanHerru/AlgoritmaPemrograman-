import math
print("tugas praktikum 2 algoritma dasar")

a = float(input("masukan nilai a = "))
b = float(input("masukan nilai b = "))

print("ini penjumlahan menggunakan math module : ",math.pow(a, b))

hasil  = a + b
print("jumlah a ditambah b adalah",hasil)

hasil = a - b
print("jumlah a dikurang b adalah",hasil)

hasil = a * b
print("jumlah a dikali b adalah",hasil)

hasil = a / b
print("jumlah a dibagi b adalah",hasil)

hasil = a % b
print("jumlah sisa pembagian a dan b adalah",hasil)

hasil = math.log10(a)
print("jumlah log a dan b adalah",hasil)

hasil = a ** b
print("jumlah a pangkat b adalah",hasil)
